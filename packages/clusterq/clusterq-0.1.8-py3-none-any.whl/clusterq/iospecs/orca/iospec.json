   ],

   "outputfiles": [
      "gbw",
      "out",
      "opt",
      "xyz",
      "trj",
      "prop",
      "engrad",
      "property"
   ],

   "prescript": [
      "sed -i '/\\s*%pal\\s\\s*end/Id;1i'\"%pal nprocs $nproc end\" $inp"
   ]
