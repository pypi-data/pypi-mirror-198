{
   "parallelib": "standalone",
   "stdoutfile": "out",

   "conflicts": {
      "not inp": "Debe existir un archivo inp"
   },

   "filekeys": {
      "inp": "job.inp",
      "out": "job.out",
      "opt": "job.opt",
      "xyz": "job.xyz",
      "trj": "job.trj",
      "gbw": "job.gbw",
      "prop": "job.prop",
      "engrad": "job.engrad",
      "property": "job_property.txt"
   },

   "filevars": {
      "inp": "inp"
   },

   "inputfiles": [
      "inp"
   ],

   "posargs": [
