@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix ex:  <http://example.org/ontology#>.
@base <http://example.org/data/>.

print("test1")
def test1(t=?var):
    print(t.n3())    

test1()
test1(g{ [] <p> 2 })
test1(t=<p>)

print("test2")
def test2(t=ex:hello):
    print(t.n3())
    
test2()
test2(ex:Sensor)
test2(t=ex:Sensor)


print("test3")
def test3(g=g{ [] <p> 1} ):
    g.print()
    
test3()
test3(g{ [] <p> 2 })

